package String;

/**
 * @author j.yadav
 *
 */
public class ReverseWordsInString {


	public static String reverseWords(String input) {
		
		String[] result = input.split(" ");
		
		for(int i=0;i<result.length;i++) {
			System.out.println(result[i] + "\n");
		}
		
		return result[0];
		
	}


}
