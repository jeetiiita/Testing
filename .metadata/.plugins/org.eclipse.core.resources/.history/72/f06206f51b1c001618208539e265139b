import org.springframework.stereotype.Service;

import com.clicktable.support.model.Tax;

@Service
public interface TaxService {
	

	Tax create(Tax tax);
	
	List<Tax> getTax(Map<String, Object> params);
	
	boolean disable(Tax tax);

}